{
  "version": 2,
  "crons": [
    {
      "path": "/api/update",
      "schedule": "*/10 * * * *"
    }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
